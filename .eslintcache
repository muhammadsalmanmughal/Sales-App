ole.log('update E', e)\n    }\n    const checkCustomer = () => {\n        if (isCustomer) {\n            return <TableDiv>\n                <Table>\n                    <TableHead>\n                        <TableHeading>BusinessName</TableHeading>\n                        <TableHeading>BillToAddress</TableHeading>\n                        <TableHeading>Phone</TableHeading>\n                        <TableHeading>Email</TableHeading>\n                        {/* <TableHeading>State</TableHeading> */}\n                        <TableHeading>City</TableHeading>\n                        {/* <TableHeading>Res. Name</TableHeading> */}\n                        {/* <TableHeading>Res. Phone</TableHeading> */}\n                        {/* <TableHeading>SecondaryPhone</TableHeading> */}\n                        {/* <TableHeading>PostalCode</TableHeading> */}\n                    </TableHead>\n                    <TableBody>\n                        {allCustomers &&\n                            allCustomers.map((customer, index) => {\n                                return (\n                                    <TableRow>\n                                        <TableData>[{customer.businessName}]</TableData>\n                                        <TableData>[{customer.billToAddress}]</TableData>\n                                        <TableData>[{customer.phone}]</TableData>\n                                        <TableData>[{customer.email}]</TableData>\n                                        {/* <TableData>[{customer.state}]</TableData> */}\n                                        <TableData>[{customer.city}]</TableData>\n                                        {/* <TableData>[{customer.responsibleName}]</TableData>\n                                        <TableData>[{customer.responsiblePhone}]</TableData>\n                                        <TableData>[{customer.secondaryPhone}]</TableData>\n                                        <TableData>[{customer.postalCode}]</TableData> */}\n                                        <TableData>\n                                            <button onClick={() => updateCustomer(customer)}>\n                                                Update customer\n                                            </button>\n                                        </TableData>\n                                        <TableData>\n                                            <button onClick={() =>\n                                                history.push(`/home/vendor-details/${customer.compId}/${'Customer'}`)\n                                            } >\n                                                customer Details\n                                            </button>\n                                        </TableData>\n                                    </TableRow>\n                                );\n                            })}\n                    </TableBody>\n                </Table>\n            </TableDiv>\n        }\n        return <EmptyDiv> <Empty/> </EmptyDiv>\n        \n\n    }\n    useEffect(() => {\n        getAllCustomers()\n    }, [])\n\n    console.log('All Customers', allCustomers);\n    return (\n        <div>\n{checkCustomer()}\n\n        </div>\n    )\n}\nexport default AllCustomers","/home/salman/Salman/My Data/university/FYP 2/sales-app/src/components/AllCustomers/style/index.js",[],"/home/salman/Salman/My Data/university/FYP 2/sales-app/src/components/PurchaseRequisition/purchaseRequisition.js",[],"/home/salman/Salman/My Data/university/FYP 2/sales-app/src/components/RFQ/rfq.js",[],"/home/salman/Salman/My Data/university/FYP 2/sales-app/src/components/PurchaseOrder/purchaseOrder.js",[],"/home/salman/Salman/My Data/university/FYP 2/sales-app/src/components/Details/details.js",["195","196"],{"ruleId":"197","replacedBy":"198"},{"ruleId":"199","replacedBy":"200"},{"ruleId":"201","severity":1,"message":"202","line":6,"column":3,"nodeType":"203","messageId":"204","en